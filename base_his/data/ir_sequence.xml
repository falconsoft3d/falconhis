<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- medical.record -->
        <record id="seq_medical_record" model="ir.sequence">
            <field name="name">Medical Record</field>
            <field name="code">medical.record</field>
            <field name="prefix">MRC/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
        <!-- medical.action -->
        <record id="seq_medical_action" model="ir.sequence">
            <field name="name">Medical Action</field>
            <field name="code">medical.action</field>
            <field name="prefix">MAC/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
        <!-- patient.movement -->
        <record id="seq_patient_movement" model="ir.sequence">
            <field name="name">Patient Movement</field>
            <field name="code">patient.movement</field>
            <field name="prefix">PMV/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
        <!-- his.mass.billing -->
        <record id="seq_his_mass_billing" model="ir.sequence">
            <field name="name">HIS Mass Billing</field>
            <field name="code">his.mass.billing</field>
            <field name="prefix">HMB/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>